{
  "name": "@swup/cli",
  "version": "5.0.0",
  "description": "Swup CLI for plugin generation, site validation and local development",
  "type": "module",
  "author": {
    "name": "Georgy Marchuk",
    "email": "gmarcuk@gmail.com",
    "url": "https://gmrchk.com/"
  },
  "bin": {
    "swup": "./bin/run"
  },
  "homepage": "https://github.com/swup/cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "swup/cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2.0.0",
    "@oclif/plugin-help": "^5.0.0",
    "git-clone": "^0.2.0",
    "listr2": "^6.6.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@oclif/test": "^2.5.3",
    "@types/chai": "^4.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.50",
    "chai": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9.0.0",
    "oclif": "^3.15.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "swup",
    "dirname": "swup",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/swup/cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
